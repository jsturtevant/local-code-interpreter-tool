# Environment variables for Local Code Interpreter Tool
# Copy this file to .env and fill in your values

# =============================================================================
# Option 1: OpenAI (api.openai.com)
# =============================================================================
OPENAI_API_KEY=""
OPENAI_RESPONSES_MODEL_ID="gpt-4o-mini"

# =============================================================================
# Option 2: Azure AI Foundry (Azure OpenAI)
# =============================================================================
# Use `az login` to authenticate with Azure CLI, OR provide AZURE_OPENAI_API_KEY
# =============================================================================
AZURE_OPENAI_ENDPOINT=https://<your deployment>.openai.azure.com/
AZURE_OPENAI_RESPONSES_DEPLOYMENT_NAME=gpt-4o
#AZURE_OPENAI_API_KEY=<for local docker dev>

